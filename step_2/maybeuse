for (let i =shoeguess.length-1; i>0;i--){
  const u=Math.floor(Math.random()*(i+1));
  [shoeguess[i],shoeguess[u],shoeguess[i]]
 }

 for (let i=0; i<shoeguess.length;i++){
  console.log(shoeguess[i]);
 }

